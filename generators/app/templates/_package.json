{
  "name": "<%= projectname_slugged %>",
  "description": "<%= projectdesc %>",
  "version": "0.1.0",
  "homepage": "https://github.com/<%= username %>/<%= projectname_slugged %>",
  "author": {
    "name": "<%= authorname %>",
    "url": "<%= authorurl %>",
    "email": "<%= authoremail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= username %>/<%= projectname_slugged %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= username %>/<%= projectname_slugged %>/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/<%= username %>/<%= projectname_slugged %>/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "command line"
  ],
  "preferGlobal": true,
  "bin": {
    "<%= cliname %>": "cli.js"
  },
  "main": "index.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "test": "xo --space && node test.js"
  },
  "dependencies": {
    "bluebird": "^2.10.0",
    "chalk": "^1.0.0",
    "meow": "^3.1.0"
  },
  "devDependencies": {
    "ava": "0.0.4",
    "xo": "*"
  }
}
